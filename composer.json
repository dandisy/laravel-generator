{
    "name": "dandisy/laravel-generator",
    "description": "Webcore Laravel Generator",
    "keywords": [
        "webcore",
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dandi Setiyawan",
            "email": "dandi@redbuzz.co.id"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0",
        "prettus/l5-repository": "~2.6",
        "laracasts/flash": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Webcore\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Webcore\\Generator\\WebcoreGeneratorServiceProvider"
            ]
        }
    }    
}
