{
    "name": "dandisy/laravel-generator",
    "description": "Webcore Laravel Generator",
    "keywords": [
        "webcore",
        "laravel",
        "api",
        "crud",
        "generator",
        "swagger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dandi Setiyawan",
            "email": "dandi@redbuzz.co.id"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.0|~6.0|~7.0|~8.0|~9.0",
        "prettus/l5-repository": "~2.6",
        "laracasts/flash": "~3.0",
        "dandisy/swagger-generator": "^1.1",
        "laravelcollective/html": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webcore\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Webcore\\Generator\\WebcoreGeneratorServiceProvider"
            ]
        }
    }    
}
